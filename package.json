{
  "name": "jsSync",
  "version": "1.0.0",
  "description": "Aims to provide a simple way for synchronizing objects from various sources through serialization / deserialization",
  "main": "lib/index.js",
  "scripts": {
    "flow": "flow",
    "babel": "babel",
    "build": "babel src/ -d lib/",
    "test": "mocha --require babel-core/register ./test/**/*.spec.js",
    "prepack": "npm run prepack:babel && npm run prepack:flow",
    "prepack:babel": "babel src/ -d lib",
    "prepack:flow": "flow-copy-source src lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cetzcher/jsSync.git"
  },
  "keywords": [
    "js",
    "flow",
    "sync",
    "serialization",
    "deserialization",
    "decorator"
  ],
  "babel": {
    "presets": [
      "flow"
    ]
  },
  "files": [
    "lib"
  ],
  "author": "Pierre Rieger",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Cetzcher/jsSync/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-flow": "^6.23.0",
    "chai": "^4.1.2",
    "chai-things": "^0.2.0",
    "flow-bin": "^0.80.0",
    "flow-copy-source": "^2.0.2",
    "mocha": "^5.2.0"
  }
}